<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context=".ReservationActivity"
    android:background="@drawable/background_paper">


    <ImageView
        android:id="@+id/titleMask"
        android:layout_width="411dp"
        android:layout_height="136dp"
        android:layout_marginTop="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/setting_title_masking" />

    <ImageView
        android:id="@+id/usnigBoxImg"
        android:layout_width="300dp"
        android:layout_height="100dp"
        android:layout_marginBottom="56dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/washerImg05"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@color/shader_color" />

    <TextView
        android:id="@+id/statusText"
        android:layout_width="300dp"
        android:layout_height="100dp"
        android:fontFamily="@font/nanumbaeeunhae"
        android:gravity="center_vertical|center"
        android:text="1번 세탁기 사용중 :)"
        android:textSize="50dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/usnigBoxImg"
        app:layout_constraintEnd_toEndOf="@+id/usnigBoxImg"
        app:layout_constraintStart_toStartOf="@+id/usnigBoxImg"
        app:layout_constraintTop_toTopOf="@+id/usnigBoxImg" />

    <ImageView
        android:id="@+id/clothLine2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="@+id/clothLine1"
        app:layout_constraintEnd_toEndOf="@+id/washerImg03"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/titleMask"
        app:layout_constraintVertical_bias="0.607"
        app:srcCompat="@drawable/clothes_line" />

    <ImageView
        android:id="@+id/clothLine1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="@+id/usnigBoxImg"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/titleMask"
        app:srcCompat="@drawable/clothes_line" />

    <TextView
        android:id="@+id/setting_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_vertical"
        android:fontFamily="@font/nanumbaeeunhae"
        android:text="세탁기 사용현황"
        android:textSize="60sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/titleMask"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/titleMask"
        app:layout_constraintTop_toTopOf="@+id/titleMask" />

    <ImageView
        android:id="@+id/washerImg02"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginTop="72dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintEnd_toStartOf="@+id/washerImg03"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/washerTimeText2"
        app:layout_constraintTop_toBottomOf="@+id/titleMask"
        app:srcCompat="@drawable/cloth_clean" />

    <ImageView
        android:id="@+id/washerImg01"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginTop="60dp"
        app:layout_constraintEnd_toEndOf="@+id/washerTimeText1"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/titleMask"
        app:srcCompat="@drawable/cloth_clean" />

    <ImageView
        android:id="@+id/washerImg03"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginTop="45dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/titleMask"
        app:srcCompat="@drawable/cloth_clean" />

    <ImageView
        android:id="@+id/washerImg04"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginTop="80dp"
        app:layout_constraintEnd_toStartOf="@+id/washerImg05"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/washerTimeText4"
        app:layout_constraintTop_toBottomOf="@+id/clothLine2"
        app:srcCompat="@drawable/cloth_clean" />

    <ImageView
        android:id="@+id/washerImg05"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginTop="90dp"
        android:layout_marginEnd="12dp"
        app:layout_constraintEnd_toStartOf="@+id/washerImg06"
        app:layout_constraintTop_toBottomOf="@+id/clothLine2"
        app:srcCompat="@drawable/cloth_clean" />

    <ImageView
        android:id="@+id/washerImg06"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginTop="64dp"
        app:layout_constraintEnd_toEndOf="@+id/washerTimeText6"
        app:layout_constraintTop_toBottomOf="@+id/clothLine2"
        app:srcCompat="@drawable/cloth_clean" />

    <TextView
        android:id="@+id/washerTimeText4"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_weight="1"
        android:fontFamily="@font/nanumbaeeunhae"
        android:gravity="center_vertical|center"
        android:text="졸업하기 싫당"
        android:textColor="#000000"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/washerImg04"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/washerImg04"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/washerTimeText6"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_weight="1"
        android:fontFamily="@font/nanumbaeeunhae"
        android:gravity="center_vertical|center"
        android:text="아 노래 좋당"
        android:textColor="#000000"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/washerImg06"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/washerImg06"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/washerTimeText5"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_weight="1"
        android:fontFamily="@font/nanumbaeeunhae"
        android:gravity="center_vertical|center"
        android:text="맛난거 먹고싶당"
        android:textColor="#000000"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/washerImg05"
        app:layout_constraintEnd_toEndOf="@+id/washerImg05"
        app:layout_constraintStart_toStartOf="@+id/washerImg05"
        app:layout_constraintTop_toTopOf="@+id/washerImg05" />

    <TextView
        android:id="@+id/washerTimeText3"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_weight="1"
        android:fontFamily="@font/nanumbaeeunhae"
        android:gravity="center_vertical|center"
        android:text="1등하고 싶당"
        android:textColor="#000000"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/washerImg03"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/washerImg03"
        app:layout_constraintTop_toTopOf="@+id/washerImg03" />

    <TextView
        android:id="@+id/washerTimeText2"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginTop="80dp"
        android:layout_marginEnd="8dp"
        android:layout_weight="1"
        android:fontFamily="@font/nanumbaeeunhae"
        android:gravity="center_vertical|center"
        android:text="연애하고 싶당"
        android:textColor="#000000"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/washerImg02"
        app:layout_constraintEnd_toStartOf="@+id/washerImg03"
        app:layout_constraintTop_toBottomOf="@+id/titleMask" />

    <TextView
        android:id="@+id/washerTimeText1"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginTop="32dp"
        android:layout_weight="1"
        android:fontFamily="@font/nanumbaeeunhae"
        android:gravity="center_vertical|center"
        android:text="아 집가고싶당"
        android:textColor="#000000"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/washerImg01"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/titleMask"
        app:layout_constraintVertical_bias="1.0" />

    <ImageView
        android:id="@+id/loading_background"
        android:layout_width="999dp"
        android:layout_height="999dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/loading_background" />

    <ImageView
        android:id="@+id/loading_icon"
        android:layout_width="122dp"
        android:layout_height="123dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/loading_icon" />

    <TextView
        android:id="@+id/loadingText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:accessibilityTraversalAfter="@id/loadingText"
        android:fontFamily="@font/nanumbaeeunhae"
        android:text="로딩중... 조금만 기다려주세요!"
        android:textColor="#A3A3A3"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/loading_icon"
        app:layout_constraintVertical_bias="0.0" />

</androidx.constraintlayout.widget.ConstraintLayout>
